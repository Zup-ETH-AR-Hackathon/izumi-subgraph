{
  "name": "izi-swap",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --product hosted-service izumifinance/iziswapsubgraph",
    "auth": "graph auth https://thegraph.com/hosted-service/subgraph/izumifinance/iziswapsubgraph XXX_AccessToken",

    "create-local-bsc": "graph create --node http://localhost:8020/ --network bsc izi-swap-bsc",
    "remove-local-bsc": "graph remove --node http://localhost:8020/ --network bsc izi-swap-bsc",
    "deploy-local-bsc": "graph deploy --node http://localhost:8020/ --network bsc--ipfs http://localhost:5001 izi-swap-bsc",

    "create-local-manta": "graph create --node http://localhost:8020/ --network manta izi-swap-manta",
    "remove-local-manta": "graph remove --node http://localhost:8020/ --network manta izi-swap-manta",
    "deploy-local-manta": "graph deploy --node http://localhost:8020/ --network manta --ipfs http://localhost:5001 izi-swap-manta",

    "create-local-mantle": "graph create --node http://localhost:8020/ --network mantle izi-swap-mantle",
    "remove-local-mantle": "graph remove --node http://localhost:8020/ --network mantle izi-swap-mantle",
    "deploy-local-mantle": "graph deploy --node http://localhost:8020/ --network mantle --ipfs http://localhost:5001 izi-swap-mantle"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.32.0",
    "@graphprotocol/graph-ts": "0.27.0"
  }
}
